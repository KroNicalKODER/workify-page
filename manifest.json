{
    "name": "My Extension",
    "description": "This is a simple example extension",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  